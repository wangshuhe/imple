{
  "target": "bmv2",
  "p4info": "build/switch.p4.p4info.txt",
  "bmv2_json": "build/switch.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv6_exact",
      "match": {
        "hdr.ipv6.dstAddr": "1000:0:0:0:0:0:1:1"
      },
      "action_name": "MyIngress.ipv6_forward",
      "action_params": {
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv6_exact",
      "match": {
        "hdr.ipv6.dstAddr": "1000:0:0:0:0:0:2:2"
      },
      "action_name": "MyIngress.ipv6_forward",
      "action_params": {
        "port": 2
      }
    }
  ]
}
